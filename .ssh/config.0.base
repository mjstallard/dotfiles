Host *
  ForwardAgent yes
  StrictHostKeyChecking no
  Compression yes
  CompressionLevel 7
  Cipher blowfish
  LogLevel ERROR
  ServerAliveInterval 600
  UserKnownHostsFile /dev/null
Host github.com
  Hostname ssh.github.com
  Port 443
Host gitlab.com
  RSAAuthentication yes
  IdentityFile ~/.ssh/gitlab_rsa
  User mtchavez
